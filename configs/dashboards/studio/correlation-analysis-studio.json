  "dataSources": {
    "ds_deny_events": {
      "type": "ds.search",
      "options": {
        "query": "index=fw action=deny earliest=$time_picker.earliest$ latest=$time_picker.latest$ | stats count as deny_events",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Deny Events"
    },
    "ds_high_risk_ips": {
      "type": "ds.search",
      "options": {
        "query": "index=fw earliest=$time_picker.earliest$ latest=$time_picker.latest$ | lookup abuseipdb_lookup.csv ip AS src_ip OUTPUT abuse_score | where abuse_score >= 90 | stats dc(src_ip) as high_risk_ips",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "High-Risk IPs"
    },
    "ds_critical_events": {
      "type": "ds.search",
      "options": {
        "query": "index=fw severity=critical earliest=$time_picker.earliest$ latest=$time_picker.latest$ | stats count as critical_events",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Critical Events"
    },
    "ds_blocked_ips": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"auto_block=*\" earliest=$time_picker.earliest$ latest=$time_picker.latest$ | stats dc(src_ip) as blocked_ips",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Blocked IPs"
    },
    "ds_correlation_alerts": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"correlation_detection=*\" earliest=$time_picker.earliest$ latest=$time_picker.latest$ | stats count as correlation_alerts",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Correlation Alerts"
    },
    "ds_rule1": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"correlation_detection=multi_factor_threat\" earliest=$time_picker.earliest$ latest=$time_picker.latest$ event_category_filter=$event_category_filter$ action_recommendation=$action_filter$ | head 20 | table _time, src_ip, correlation_score, event_count, abuse_score, country, action_recommendation | sort -correlation_score",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Rule 1: Multi-Factor Threat"
    },
    "ds_rule1_trend": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"correlation_detection=multi_factor_threat\" earliest=$time_picker.earliest$ latest=$time_picker.latest$ | timechart span=15m avg(correlation_score) as avg_score max(correlation_score) as max_score",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Rule 1 Trend"
    },
    "ds_rule2": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"correlation_detection=repeated_high_risk\" earliest=$time_picker.earliest$ latest=$time_picker.latest$ event_category_filter=$event_category_filter$ action_recommendation=$action_filter$ | head 20 | table _time, src_ip, correlation_score, high_risk_count, total_events, action_recommendation | sort -correlation_score",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Rule 2: Repeated High-Risk"
    },
    "ds_rule2_trend": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"correlation_detection=repeated_high_risk\" earliest=$time_picker.earliest$ latest=$time_picker.latest$ | timechart span=10m avg(correlation_score) as avg_score",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Rule 2 Trend"
    },
    "ds_rule3": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"correlation_detection=weak_signal_combo\" earliest=$time_picker.earliest$ latest=$time_picker.latest$ event_category_filter=$event_category_filter$ action_recommendation=$action_filter$ | head 20 | table _time, src_ip, correlation_score, signal_count, abuse_score, event_count, unique_dst_count, action_recommendation | sort -correlation_score",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Rule 3: Weak Signal"
    },
    "ds_rule3_details": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"correlation_detection=weak_signal_combo\" earliest=$time_picker.earliest$ latest=$time_picker.latest$ | head 20 | table src_ip, has_low_abuse, has_failed_login, has_port_scan, has_multiple_targets, has_high_frequency",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Rule 3 Signal Details"
    },
    "ds_rule4": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"correlation_detection=geo_attack_pattern\" earliest=$time_picker.earliest$ latest=$time_picker.latest$ event_category_filter=$event_category_filter$ action_recommendation=$action_filter$ | head 20 | table _time, src_ip, country, correlation_score, attack_count, geo_risk, action_recommendation | sort -correlation_score",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Rule 4: Geo + Attack"
    },
    "ds_rule4_geo": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"correlation_detection=geo_attack_pattern\" earliest=$time_picker.earliest$ latest=$time_picker.latest$ | stats count by country | geom geo_countries featureIdField=country",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Rule 4 Geographic"
    },
    "ds_rule5": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"correlation_detection=time_anomaly\" earliest=$time_picker.earliest$ latest=$time_picker.latest$ event_category_filter=$event_category_filter$ action_recommendation=$action_filter$ | head 20 | table _time, src_ip, correlation_score, current_count, baseline_avg, zscore, spike_ratio, action_recommendation | sort -correlation_score",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Rule 5: Time Anomaly"
    },
    "ds_rule5_timechart": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"correlation_detection=time_anomaly\" earliest=$time_picker.earliest$ latest=$time_picker.latest$ | timechart span=5m sum(current_count) as events avg(baseline_avg) as baseline",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Rule 5 Time Chart"
    },
    "ds_rule6": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"correlation_detection=cross_event_type\" earliest=$time_picker.earliest$ latest=$time_picker.latest$ event_category_filter=$event_category_filter$ action_recommendation=$action_filter$ | head 20 | table _time, src_ip, correlation_score, attack_type_count, attack_types, total_events, action_recommendation | sort -correlation_score",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Rule 6: Cross-Event APT"
    },
    "ds_rule6_matrix": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"correlation_detection=cross_event_type\" earliest=$time_picker.earliest$ latest=$time_picker.latest$ | eval attack_list=split(attack_types, \", \") | mvexpand attack_list | stats count by src_ip, attack_list | xyseries src_ip attack_list count",
        "queryParameters": {
          "earliest": "$time_picker.earliest$",
          "latest": "$time_picker.latest$"
        }
      },
      "name": "Rule 6 Attack Matrix"
    },
    "ds_alert_history": {
      "type": "ds.search",
      "options": {
        "query": "index=summary_fw marker=\"correlation_detection=*\" earliest=-24h | stats count by marker, action_recommendation | sort -count",
        "queryParameters": {
          "earliest": "-24h",
          "latest": "now"
        }
      },
      "name": "Alert History"
    },
    "ds_alert_success_rate": {
      "type": "ds.search",
      "options": {
        "query": "index=_internal source=*scheduler.log savedsearch_name=\"Correlation_*\" | stats count(eval(result=\"success\")) as success count as total | eval success_rate=round(success/total*100, 2)",
        "queryParameters": {
          "earliest": "-24h",
          "latest": "now"
        }
      },
      "name": "Alert Success Rate"
    },
    "ds_slack_status": {
      "type": "ds.search",
      "options": {
        "query": "| inputlookup slack_alert_toggle | where alert_name=\"all\" | head 1 | eval status=if(enabled=1, \"Enabled\", \"Disabled\") | table status",
        "queryParameters": {
          "earliest": "0",
          "latest": "now"
        }
      },
      "name": "Slack Status"
    },
    "ds_rule_slack_toggles": {
      "type": "ds.search",
      "options": {
        "query": "| inputlookup slack_alert_toggle | where alert_name!=\"all\" | eval status=if(enabled=1, \"âœ… ON\", \"â›” OFF\"), toggle_link=if(enabled=1, \"[Disable]\", \"[Enable]\") | table alert_name, status, updated_by, updated_time | rename alert_name as \"Rule Name\", status as \"Slack Alert\", updated_by as \"Updated By\", updated_time as \"Last Updated\"",
        "queryParameters": {
          "earliest": "0",
          "latest": "now"
        }
      },
      "name": "Rule Slack Toggles"
    }
  },
  "defaults": {
    "dataSources": {
      "ds.search": {
        "options": {
          "queryParameters": {
            "latest": "$global_time.latest$",
            "earliest": "$global_time.earliest$"
          }
        }
      }
    }
  },
  "inputs": {
    "input_time_picker": {
      "type": "input.timerange",
      "options": {
        "token": "time_picker",
        "defaultValue": "-24h@h,now"
      },
      "title": "Time Range"
    },
    "input_event_category": {
      "type": "input.dropdown",
      "options": {
        "items": [
          {
            "label": "All Types",
            "value": "*"
          },
          {
            "label": "Multi-Factor Threat",
            "value": "multi_factor_threat"
          },
          {
            "label": "Repeated High-Risk Events",
            "value": "repeated_high_risk"
          },
          {
            "label": "Weak Signal Combination",
            "value": "weak_signal_combo"
          },
          {
            "label": "Geo + Attack Pattern",
            "value": "geo_attack_pattern"
          },
          {
            "label": "Time-Based Anomaly",
            "value": "time_anomaly"
          },
          {
            "label": "Cross-Event Type",
            "value": "cross_event_type"
          }
        ],
        "defaultValue": "*",
        "token": "event_category_filter"
      },
      "title": "Correlation Type"
    },
    "input_action_filter": {
      "type": "input.dropdown",
      "options": {
        "items": [
          {
            "label": "All Actions",
            "value": "*"
          },
          {
            "label": "Auto-Block",
            "value": "AUTO_BLOCK"
          },
          {
            "label": "Review & Block",
            "value": "REVIEW_AND_BLOCK"
          },
          {
            "label": "Monitor Only",
            "value": "MONITOR"
          }
        ],
        "defaultValue": "*",
        "token": "action_filter"
      },
      "title": "Action Recommendation"
    }
  },
