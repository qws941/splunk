[]
# secmon 사용자 전용 role로 접근 제한
# Splunk에서 secmon_role 생성 후 secmon 사용자를 이 role에 할당
access = read : [ secmon_role, admin ], write : [ admin ]
export = system

# 다른 role 추가 예시:
# access = read : [ secmon_role, security_team, admin ], write : [ admin ]

[transforms]
export = system

[savedsearches]
export = system

[macros]
export = system

[props]
export = system

[lookups]
export = system
